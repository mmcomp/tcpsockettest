<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android"
    id="com.mmcomp.bytesocketclientplugin"
    version="0.0.1">
    <name>ByteSocketClientPlugin</name>
    <description>The Bytes Socket Client Plugin for Android.</description>
    <license>Apache 2.0</license>
    <keywords>cordova,socket,client,byte</keywords>
    <repo></repo>
    <issue></issue>
    <js-module src="www/ByteSocketClientPlugin.js" name="ByteSocketClientPlugin">
        <clobbers target="ByteSocketClientPlugin" />
    </js-module>
    <!-- android -->
    <platform name="android">
        <config-file target="res/xml/config.xml" parent="/*">
            <feature name="ByteSocketClientPlugin" >
                <param name="android-package" value="ByteSocketClientPlugin"/>
            </feature>
        </config-file>
        <source-file src="src/android/ByteSocketClientPlugin.java" target-dir="src/" />
    </platform>
</plugin>
